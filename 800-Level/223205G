#include <bits/stdc++.h>

using namespace std;

void maxmin(int n) {
    vector < int > v(n);
    for (int i = 0; i < n; i++) {
        cin >> v[i];


    }
    int min = v[0];
    int max = v[0];


    for (int i = 1; i < n; i++) {
        if (min > v[i]) {
            min = v[i];
        }
        if (max < v[i]) {
            max = v[i];
        }

    }
    cout << min << " " << max << endl;
}

int main() {

    int n;
    cin >> n;
    maxmin(n);

}