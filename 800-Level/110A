#include<bits/stdc++.h>
using namespace std;
int main()
{

    long long n,count;
    cin>>n;

    vector<int>v;
    while(n>0)
    {
        v.push_back(n%10);
        n=n/10;
    }
    count=0;

    for(int i=0; i<v.size(); i++)
    {
        if(v[i]==4 || v[i]==7)
        {
            count++;
        }


    }

    if(count==4 || count==7)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }

}